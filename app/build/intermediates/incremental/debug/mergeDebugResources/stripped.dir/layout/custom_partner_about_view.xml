<?xml version="1.0" encoding="utf-8"?>

                                                              
                                                        

    

           

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" android:tag="layout/custom_partner_about_view_0" xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">

        <android.textview.CustomTextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/please_tell_us_more_how_you_want_to_partner_with_us_and_why"
            android:textColor="@color/black"
            android:textAlignment="viewStart"
            android:textSize="@dimen/_16sdp"
            app:font_name="@string/sfdiaply_bold" />

        <android.edittext.CustomEditText
            android:id="@+id/et_about"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15sdp"
            android:background="@drawable/white_rounded_corner_5"
            android:gravity="start"
            android:textAlignment="viewStart"
            android:hint="@string/i_m_looking_to_become_a_partner_influencebird"
            android:imeOptions="actionNext"
            android:padding="@dimen/_10sdp"
            android:textColor="@color/black"
            android:textSize="@dimen/_13sdp"
            tools:ignore="TextContrastCheck" />
    </LinearLayout>
         