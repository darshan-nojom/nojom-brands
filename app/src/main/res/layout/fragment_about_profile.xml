<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical">

            <android.textview.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:textAlignment="viewStart"
                android:text="@string/best_gigs_rank"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                app:font_name="@string/sf_protext_bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="horizontal">

                <android.textview.CustomTextView
                    android:id="@+id/txtAllService"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp"
                    app:font_name="@string/sf_protext_bold" />

                <android.textview.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="@string/all_services_ul"
                    android:textColor="@color/blue_green"
                    android:textSize="@dimen/_12sdp"
                    app:font_name="@string/sf_protext_medium" />

                <android.textview.CustomTextView
                    android:id="@+id/txtWritingService"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:gravity="center_vertical"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12sdp"
                    app:font_name="@string/sf_protext_bold" />

                <android.textview.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="@string/writing_services"
                    android:textColor="@color/blue_green"
                    android:textSize="@dimen/_12sdp"
                    app:font_name="@string/sf_protext_medium" />

            </LinearLayout>

            <android.textview.CustomTextView
                android:id="@+id/tv_headlineTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:textAlignment="viewStart"
                android:text="@string/headline"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_headline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:id="@+id/tv_about_meTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/about_me"
                android:textColor="@color/black"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <com.nojom.client.util.ReadMoreTextView
                android:id="@+id/tv_about_me"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:readMoreTextColor="@color/blue_green"
                app:trimLines="3"
                app:trimMode="trimModeLine" />

            <android.textview.CustomTextView
                android:id="@+id/tv_profAdd_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/professional_address"
                android:textColor="@color/black"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_profAdd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textSize="@dimen/_12sdp"
                android:textAlignment="viewStart"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:id="@+id/tv_location_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/location"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:id="@+id/tv_phone_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAlignment="viewStart"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/phone"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:id="@+id/tv_email_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/email"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:textAlignment="viewStart"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textSize="@dimen/_12sdp"
                android:textAlignment="viewStart"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:id="@+id/tv_website_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/website"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_bold" />

            <android.textview.CustomTextView
                android:id="@+id/tv_website"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                android:visibility="gone"
                app:font_name="@string/sftext_regular" />

            <android.textview.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="@string/language"
                android:textColor="@color/black"
                android:textAlignment="viewStart"
                android:textSize="@dimen/_12sdp"
                app:font_name="@string/sftext_bold" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_languages"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp" />
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>